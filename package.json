  "name": "webpack-builder",
  "version": "0.0.1",
  "description": "Front-end development framework base webpack5 vue3 typescript",
  "author": "blaze",
  "scripts": {
    "dev": "node --max-old-space-size=8192 --trace-deprecation bin/webpack/serve.js",
    "start": "npm-run-all --parallel dev chrome",
    "chrome": "node bin/tools/chromeLaunch.js",
    "build": "node --max-old-space-size=8192 --trace-deprecation bin/webpack/build.js",
    "inspect": "node bin/webpack/helper/inspect.js",
    "lint": "eslint --ext .js,.vue --fix src",
    "githooks": "node bin/tools/gitHooks.js",
    "preinstall": "node bin/tools/preinstall.js",
    "postinstall": "node bin/tools/postinstall.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run githooks --commit-msg",
      "pre-push": "npm run githooks --pre-push"
    }
